<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.koom.app">

    <uses-permission android:name="android.permission.INTERNET" />

<!--    usesCleartextTraffic untuk Pie keatas-->

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_mobile"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">

        <activity android:name="id.koom.app.AmbilGambar" />
        <activity android:name="id.koom.app.MainApp" />
        <activity android:name="id.koom.app.StatusPembayaran" />
        <activity android:name="id.koom.app.MetodePembayaran" />
        <activity android:name="id.koom.app.Pesan" />
        <activity android:name="id.koom.app.Order" />
        <activity android:name="id.koom.app.Profile" />
        <activity android:name="id.koom.app.Slider" />
        <activity android:name="id.koom.app.PanduanBayar" />
        <activity android:name="id.koom.app.PanduanPengajuan" />
        <activity android:name="id.koom.app.Bantuan" />
        <activity android:name="id.koom.app.PaymentSuccessDialog" />
        <activity android:name="id.koom.app.BerkasPoto" />
        <activity android:name="id.koom.app.DialogBerkas" />
        <activity android:name="id.koom.app.FormPengajuanTambahan" />
        <activity android:name="id.koom.app.FormPengajuan" />
        <activity android:name="id.koom.app.AlamatPengirim" />
        <activity android:name="id.koom.app.ShopCartCard" />
        <activity android:name="id.koom.app.ShopProductDetails" />
        <activity android:name="id.koom.app.Toolbar" />
        <activity android:name="id.koom.app.ShopProductGrid" />
        <activity android:name="id.koom.app.Dashboard" />
        <activity android:name="id.koom.app.Kode_OTR" />
        <activity android:name="id.koom.app.SignUp" />
        <activity android:name="id.koom.app.Login" />
        <activity android:name="id.koom.app.SlideLayout" />
        <activity android:name="id.koom.app.SplashScreenSlide" />
        <activity android:name="id.koom.app.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="id.koom.app.MainActivity" />

        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id" />

        <service android:name="id.koom.app.rest.MyMessageService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
        <meta-data
            android:name="com.google.firebase.messaging.default_notification_icon"
            android:resource="@drawable/ic_notifications_black_24dp" />
        <meta-data
            android:name="com.google.firebase.messaging.default_notification_color"
            android:resource="@color/colorAccent" />

        <uses-library android:name="org.apache.http.legacy" android:required="false"/>
    </application>



</manifest>